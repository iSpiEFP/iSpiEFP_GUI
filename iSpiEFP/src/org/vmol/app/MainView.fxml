<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="root" prefHeight="550.0" prefWidth="550.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.vmol.app.MainViewController">
   <top>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <MenuBar>
              <menus>
                <Menu mnemonicParsing="false" text="Viewer">
                  <items>
                    <MenuItem mnemonicParsing="false" onAction="#openFile" text="Open" />
                        
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Gamess">
                  <items>
                    
                    <MenuItem mnemonicParsing="false" onAction="#openGamessSubmissionHistoryWindow" text="Submission History.." />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Libefp">
                  <items>
                    
                    <MenuItem mnemonicParsing="false" onAction="#openSubmissionHistoryWindow" text="Submission History.." />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Server">
                  <items>
                    <MenuItem mnemonicParsing="false" onAction="#openServersListWindow" text="Edit Servers.." />
                  </items>
                </Menu>
                
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
            <Pane prefHeight="55.0" prefWidth="200.0">
               <children>
                  <ToggleButton layoutX="14.0" layoutY="5.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="45.0" text="Halo On" />
                  <ToggleButton layoutX="76.0" layoutY="5.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="45.0" text="Fragmenation" />
                  <Button layoutX="139.0" layoutY="5.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="70.0" text="undo" />
                  <ToggleButton layoutX="221.0" layoutY="5.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="45.0" text="play_pause" />
                  <Button layoutX="283.0" layoutY="5.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="45.0" text="show_console" />
                  <Button layoutX="343.0" layoutY="5.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="80.0" text="Submit" />
               </children>
            </Pane>
         </children>
      </VBox>
   </top>
   <bottom>
      <HBox prefHeight="0.0" prefWidth="550.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="iSpiEFP">
               <HBox.margin>
                  <Insets bottom="6.0" left="6.0" right="6.0" top="6.0" />
               </HBox.margin>
            </Label>
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <padding>
            <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
         </padding>
      </HBox>
   </bottom>
   <center>
      <SplitPane dividerPositions="0.5" prefHeight="160.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <items>
            <ListView prefHeight="200.0" prefWidth="200.0" />
            <SplitPane dividerPositions="0.5" prefHeight="160.0" prefWidth="200.0">
               <items>
                  <Pane prefHeight="200.0" prefWidth="200.0" />
                  <SplitPane dividerPositions="0.5" orientation="VERTICAL" prefHeight="200.0" prefWidth="160.0">
                     <items>
                        <Pane prefHeight="200.0" prefWidth="200.0" />
                        <ListView prefHeight="200.0" prefWidth="200.0" />
                     </items>
                  </SplitPane>
               </items>
            </SplitPane>
        </items>
      </SplitPane>
   </center>
</BorderPane>
